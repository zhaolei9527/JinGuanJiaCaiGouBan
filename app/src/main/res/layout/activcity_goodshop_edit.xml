<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bgfff"
    android:fitsSystemWindows="true"
    android:focusable="true"
    android:focusableInTouchMode="true"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/x37"
        android:background="@color/bgtitle"
        android:visibility="visible">

        <FrameLayout
            android:id="@+id/fl_back"
            android:layout_width="@dimen/x30"
            android:layout_height="@dimen/x30"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/x10">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_gravity="center"
                android:background="@mipmap/back" />

        </FrameLayout>

        <TextView
            android:id="@+id/tv_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="商品_新增"
            android:textColor="@color/white"
            android:textSize="@dimen/x15" />

        <LinearLayout
            android:id="@+id/ll_Save_GOODSHOP"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:gravity="center_vertical">

            <ImageView
                android:layout_width="@dimen/x20"
                android:layout_height="@dimen/x20"
                android:background="@mipmap/save" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_marginLeft="@dimen/x5"
                android:layout_marginRight="@dimen/x5"
                android:text="保存"
                android:textColor="@color/white"
                android:textSize="@dimen/x13" />

        </LinearLayout>

    </RelativeLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/x5"
                android:layout_marginTop="@dimen/x10"
                android:layout_marginRight="@dimen/x15"
                android:layout_marginBottom="@dimen/x5"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:padding="@dimen/x5">

                <TextView
                    android:layout_width="@dimen/x50"
                    android:layout_height="@dimen/x15"
                    android:layout_centerVertical="true"
                    android:text="供货商："
                    android:textSize="@dimen/x11" />

                <EditText
                    android:id="@+id/et_GHS"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/y23"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/x10"
                    android:layout_weight="1"
                    android:background="@drawable/bg_gonghuoshang_edit"
                    android:imeOptions="actionSearch"
                    android:paddingLeft="@dimen/x5"
                    android:singleLine="true"
                    android:textColor="@color/text333"
                    android:textSize="@dimen/x13" />

                <ImageView
                    android:id="@+id/img_selelte_gonghuoshang"
                    android:layout_width="@dimen/x15"
                    android:layout_height="@dimen/x10"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/x5"
                    android:background="@mipmap/down" />

            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/x10"
                android:layout_marginRight="@dimen/x20"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">

                    <TextView
                        android:layout_width="@dimen/x50"
                        android:layout_height="wrap_content"
                        android:text="主营分类"
                        android:textSize="@dimen/x11" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/bg_gonghuoshang_edit"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <EditText
                            android:id="@+id/yiji"
                            android:layout_width="0dp"
                            android:layout_height="@dimen/y23"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/x5"
                            android:layout_weight="1"
                            android:background="@null"
                            android:gravity="center_vertical"
                            android:hint=""
                            android:singleLine="true"
                            android:textColor="@color/text333"
                            android:textColorHint="@color/text999"
                            android:textSize="@dimen/x12" />

                        <FrameLayout
                            android:id="@+id/ll_pro_fl1"
                            android:layout_width="@dimen/x25"
                            android:layout_height="@dimen/x25"
                            android:visibility="visible">

                            <ImageView
                                android:layout_width="@dimen/x15"
                                android:layout_height="@dimen/x10"
                                android:layout_gravity="center"
                                android:background="@mipmap/down" />

                        </FrameLayout>

                    </LinearLayout>


                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/x10"
                android:layout_marginTop="@dimen/y5"
                android:layout_marginRight="@dimen/x20"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">

                    <TextView
                        android:layout_width="@dimen/x50"
                        android:layout_height="wrap_content"
                        android:text="二分类"
                        android:textSize="@dimen/x11" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/bg_gonghuoshang_edit"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <EditText
                            android:id="@+id/erji"
                            android:layout_width="0dp"
                            android:layout_height="@dimen/y23"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/x5"
                            android:layout_weight="1"
                            android:background="@null"
                            android:gravity="center_vertical"
                            android:hint=""
                            android:singleLine="true"
                            android:textColor="@color/text333"
                            android:textColorHint="@color/text999"
                            android:textSize="@dimen/x12" />

                        <FrameLayout
                            android:id="@+id/ll_pro_fl2"
                            android:layout_width="@dimen/x25"
                            android:layout_height="@dimen/x25"
                            android:visibility="visible">

                            <ImageView
                                android:layout_width="@dimen/x15"
                                android:layout_height="@dimen/x10"
                                android:layout_gravity="center"
                                android:background="@mipmap/down" />

                        </FrameLayout>

                    </LinearLayout>


                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/x10"
                android:layout_marginTop="@dimen/y5"
                android:layout_marginRight="@dimen/x20"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">

                    <TextView
                        android:layout_width="@dimen/x50"
                        android:layout_height="wrap_content"
                        android:text="三分类"
                        android:textSize="@dimen/x11" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/bg_gonghuoshang_edit"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <EditText
                            android:id="@+id/sanji"
                            android:layout_width="0dp"
                            android:layout_height="@dimen/y23"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/x5"
                            android:layout_weight="1"
                            android:background="@null"
                            android:gravity="center_vertical"
                            android:hint=""
                            android:singleLine="true"
                            android:textColor="@color/text333"
                            android:textColorHint="@color/text999"
                            android:textSize="@dimen/x12" />

                        <FrameLayout
                            android:id="@+id/ll_pro_fl3"
                            android:layout_width="@dimen/x25"
                            android:layout_height="@dimen/x25"
                            android:visibility="visible">

                            <ImageView
                                android:layout_width="@dimen/x15"
                                android:layout_height="@dimen/x10"
                                android:layout_gravity="center"
                                android:background="@mipmap/down" />

                        </FrameLayout>


                    </LinearLayout>


                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/x10"
                android:layout_marginTop="@dimen/y5"
                android:layout_marginRight="@dimen/x20"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">

                    <TextView
                        android:layout_width="@dimen/x50"
                        android:layout_height="wrap_content"
                        android:text="商品编号"
                        android:textSize="@dimen/x11" />


                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/bg_gonghuoshang_edit"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/tv_BH"
                            android:layout_width="0dp"
                            android:layout_height="@dimen/y23"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/x5"
                            android:layout_weight="1"
                            android:background="@null"
                            android:gravity="center_vertical"
                            android:hint=""
                            android:singleLine="true"
                            android:textColor="@color/text333"
                            android:textColorHint="@color/text999"
                            android:textSize="@dimen/x12" />

                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/x10"
                android:layout_marginTop="@dimen/y5"
                android:layout_marginRight="@dimen/x20"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">

                    <LinearLayout
                        android:layout_width="@dimen/x50"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="商品名称"
                            android:textSize="@dimen/x11" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="*"
                            android:textColor="@color/textred"
                            android:textSize="@dimen/x12" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <EditText
                            android:id="@+id/et_MC"
                            android:layout_width="0dp"
                            android:layout_height="@dimen/y23"
                            android:layout_centerVertical="true"
                            android:layout_weight="1"
                            android:background="@drawable/bg_gonghuoshang_edit"
                            android:hint="必填，不区分大小写"
                            android:paddingLeft="@dimen/x5"
                            android:singleLine="true"
                            android:textColor="@color/text333"
                            android:textColorHint="@color/text999"
                            android:textSize="@dimen/x11" />

                        <TextView
                            android:id="@+id/tv_zxing"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/x5"
                            android:text="扫二维码"
                            android:textColor="@color/bgtitle"
                            android:textSize="@dimen/x10" />

                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/x10"
                android:layout_marginTop="@dimen/y5"
                android:layout_marginRight="@dimen/x20"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">

                    <TextView
                        android:layout_width="@dimen/x50"
                        android:layout_height="wrap_content"
                        android:text="商品简码"
                        android:textSize="@dimen/x11" />


                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/bg_gonghuoshang_edit"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <EditText
                            android:id="@+id/et_JM"
                            android:layout_width="0dp"
                            android:layout_height="@dimen/y23"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/x5"
                            android:layout_weight="1"
                            android:background="@null"
                            android:hint="按名称首字母自动生成，可修改"
                            android:singleLine="true"
                            android:textColor="@color/text333"
                            android:textColorHint="@color/text999"
                            android:textSize="@dimen/x11" />

                    </LinearLayout>


                </LinearLayout>

            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/x10"
                android:layout_marginTop="@dimen/y5"
                android:layout_marginRight="@dimen/x20"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">

                    <TextView
                        android:layout_width="@dimen/x50"
                        android:layout_height="wrap_content"
                        android:text="通用码"
                        android:textSize="@dimen/x11" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/bg_gonghuoshang_edit"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <EditText
                            android:id="@+id/et_TYM"
                            android:layout_width="0dp"
                            android:layout_height="@dimen/y23"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/x5"
                            android:layout_weight="1"
                            android:background="@null"
                            android:hint="请输入6位通用码"
                            android:maxLength="6"
                            android:singleLine="true"
                            android:textColor="@color/text333"
                            android:textColorHint="@color/text999"
                            android:textSize="@dimen/x11" />

                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/x10"
                android:layout_marginTop="@dimen/y5"
                android:layout_marginRight="@dimen/x20"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">

                    <TextView
                        android:layout_width="@dimen/x50"
                        android:layout_height="wrap_content"
                        android:text="商品条码"
                        android:textSize="@dimen/x11" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <EditText
                            android:id="@+id/et_TM"
                            android:layout_width="0dp"
                            android:layout_height="@dimen/y23"
                            android:layout_centerVertical="true"
                            android:layout_weight="1"
                            android:background="@drawable/bg_gonghuoshang_edit"
                            android:inputType="phone"
                            android:paddingLeft="@dimen/x5"
                            android:singleLine="true"
                            android:textColor="@color/text333"
                            android:textColorHint="@color/text999"
                            android:textSize="@dimen/x11" />

                        <CheckBox
                            android:id="@+id/cb_Check"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:checked="true"
                            android:text="自动"
                            android:textSize="@dimen/x11" />

                    </LinearLayout>


                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/x10"
                android:layout_marginTop="@dimen/y5"
                android:layout_marginRight="@dimen/x20"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">

                    <TextView
                        android:layout_width="@dimen/x50"
                        android:layout_height="wrap_content"
                        android:text="采购单价"
                        android:textSize="@dimen/x11" />


                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/bg_gonghuoshang_edit"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <EditText
                            android:id="@+id/tv_CGDJ"
                            android:layout_width="0dp"
                            android:layout_height="@dimen/y23"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/x5"
                            android:layout_weight="1"
                            android:background="@null"
                            android:gravity="center_vertical"
                            android:hint=""
                            android:inputType="numberDecimal"
                            android:singleLine="true"
                            android:textColor="@color/text333"
                            android:textColorHint="@color/text999"
                            android:textSize="@dimen/x12" />

                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/x10"
                android:layout_marginTop="@dimen/y5"
                android:layout_marginRight="@dimen/x20"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">

                    <TextView
                        android:layout_width="@dimen/x50"
                        android:layout_height="wrap_content"
                        android:text="预售单价"
                        android:textSize="@dimen/x11" />


                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/bg_gonghuoshang_edit"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <EditText
                            android:id="@+id/tv_YSDJ"
                            android:layout_width="0dp"
                            android:layout_height="@dimen/y23"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/x5"
                            android:layout_weight="1"
                            android:background="@null"
                            android:gravity="center_vertical"
                            android:hint=""
                            android:inputType="numberDecimal"
                            android:singleLine="true"
                            android:textColor="@color/text333"
                            android:textColorHint="@color/text999"
                            android:textSize="@dimen/x12" />

                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/x10"
                android:layout_marginTop="@dimen/y5"
                android:layout_marginRight="@dimen/x20"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">

                    <TextView
                        android:layout_width="@dimen/x50"
                        android:layout_height="wrap_content"
                        android:text="单店数量"
                        android:textSize="@dimen/x11" />


                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/bg_gonghuoshang_edit"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <EditText
                            android:id="@+id/tv_DDSL"
                            android:layout_width="0dp"
                            android:layout_height="@dimen/y23"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/x5"
                            android:layout_weight="1"
                            android:background="@null"
                            android:gravity="center_vertical"
                            android:hint=""
                            android:inputType="numberDecimal"
                            android:singleLine="true"
                            android:textColor="@color/text333"
                            android:textColorHint="@color/text999"
                            android:textSize="@dimen/x12" />

                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/x10"
                android:layout_marginTop="@dimen/y5"
                android:layout_marginRight="@dimen/x20"
                android:layout_marginBottom="@dimen/x20"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:layout_width="@dimen/x50"
                        android:layout_height="wrap_content"
                        android:layout_gravity="start"
                        android:paddingTop="@dimen/x10"
                        android:text="备注"
                        android:textSize="@dimen/x11" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/bg_gonghuoshang_edit">

                        <EditText
                            android:id="@+id/et_beizhu"
                            android:layout_width="0dp"
                            android:layout_height="@dimen/y80"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/x5"
                            android:layout_weight="1"
                            android:background="@null"
                            android:gravity="start"
                            android:paddingTop="@dimen/x10"
                            android:singleLine="true"
                            android:textColor="@color/text333"
                            android:textColorHint="@color/text999"
                            android:textSize="@dimen/x12" />

                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/x5"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="@dimen/x50"
                        android:layout_height="wrap_content"
                        android:layout_gravity="start"
                        android:paddingTop="@dimen/x10"
                        android:text="商品图片"
                        android:textSize="@dimen/x11" />


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center_vertical"
                            android:paddingTop="@dimen/x10"
                            android:paddingBottom="@dimen/x10">

                            <LinearLayout
                                android:id="@+id/ll_check_photo"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <ImageView
                                    android:layout_width="@dimen/x15"
                                    android:layout_height="@dimen/x15" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="@dimen/x5"
                                    android:text="选择照片" />

                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/ll_clear_photo"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <ImageView
                                    android:layout_width="@dimen/x15"
                                    android:layout_height="@dimen/x15"
                                    android:layout_marginLeft="@dimen/x15" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="@dimen/x5"
                                    android:text="清除" />

                            </LinearLayout>

                        </LinearLayout>

                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>

            <ImageView
                android:id="@+id/img_shop"
                android:layout_width="@dimen/x180"
                android:layout_height="@dimen/x180"
                android:scaleType="centerInside"
                android:layout_gravity="center_horizontal" />

        </LinearLayout>

    </ScrollView>


</LinearLayout>
