<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bgfff"
    android:fitsSystemWindows="true"
    android:focusable="true"
    android:focusableInTouchMode="true"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/x37"
        android:background="@color/bgtitle"
        android:visibility="visible">

        <FrameLayout
            android:id="@+id/fl_back"
            android:layout_width="@dimen/x30"
            android:layout_height="@dimen/x30"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/x10">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_gravity="center"
                android:background="@mipmap/ic_launcher" />

        </FrameLayout>

        <TextView
            android:id="@+id/tv_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="供货商_新增"
            android:textColor="@color/white"
            android:textSize="@dimen/x15" />

        <LinearLayout
            android:id="@+id/ll_Save_GongHuoShang"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:gravity="center_vertical">

            <ImageView
                android:layout_width="@dimen/x30"
                android:layout_height="@dimen/x30"
                android:background="@mipmap/ic_launcher" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_marginLeft="@dimen/x5"
                android:layout_marginRight="@dimen/x5"
                android:text="保存"
                android:textColor="@color/white"
                android:textSize="@dimen/x13" />

        </LinearLayout>

    </RelativeLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/x10"
                android:layout_marginTop="@dimen/y15"
                android:layout_marginRight="@dimen/x20"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">

                    <LinearLayout
                        android:layout_width="@dimen/x50"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="区域"
                            android:textSize="@dimen/x12" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="*"
                            android:textColor="@color/textred"
                            android:textSize="@dimen/x12" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/bg_gonghuoshang_edit"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <EditText
                            android:id="@+id/et_address"
                            android:layout_width="0dp"
                            android:layout_height="@dimen/y23"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/x5"
                            android:layout_weight="1"
                            android:background="@null"
                            android:hint="请录入区域名称"
                            android:singleLine="true"
                            android:textColor="@color/text333"
                            android:textColorHint="@color/text999"
                            android:textSize="@dimen/x11" />

                        <FrameLayout
                            android:id="@+id/fl_address"
                            android:layout_width="@dimen/x25"
                            android:layout_height="@dimen/x25">

                            <ImageView
                                android:layout_width="@dimen/x15"
                                android:layout_height="@dimen/x15"
                                android:layout_gravity="center"
                                android:background="@mipmap/ic_launcher" />

                        </FrameLayout>

                    </LinearLayout>


                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/x10"
                android:layout_marginTop="@dimen/y5"
                android:layout_marginRight="@dimen/x20"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">

                    <TextView
                        android:layout_width="@dimen/x50"
                        android:layout_height="wrap_content"
                        android:text="编号"
                        android:textSize="@dimen/x12" />


                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/bg_gonghuoshang_edit"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/tv_BH"
                            android:layout_width="0dp"
                            android:layout_height="@dimen/y23"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/x5"
                            android:layout_weight="1"
                            android:background="@null"
                            android:gravity="center_vertical"
                            android:hint=""
                            android:singleLine="true"
                            android:textColor="@color/text333"
                            android:textColorHint="@color/text999"
                            android:textSize="@dimen/x12" />

                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/x10"
                android:layout_marginTop="@dimen/y5"
                android:layout_marginRight="@dimen/x20"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">

                    <TextView
                        android:layout_width="@dimen/x50"
                        android:layout_height="wrap_content"
                        android:text="通用码"
                        android:textSize="@dimen/x12" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/bg_gonghuoshang_edit"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <EditText
                            android:id="@+id/et_TYM"
                            android:layout_width="0dp"
                            android:layout_height="@dimen/y23"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/x5"
                            android:layout_weight="1"
                            android:background="@null"
                            android:hint="请输入6位通用码"
                            android:maxLength="6"
                            android:singleLine="true"
                            android:textColor="@color/text333"
                            android:textColorHint="@color/text999"
                            android:textSize="@dimen/x11" />

                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/x10"
                android:layout_marginTop="@dimen/y5"
                android:layout_marginRight="@dimen/x20"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">

                    <LinearLayout
                        android:layout_width="@dimen/x50"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="名称"
                            android:textSize="@dimen/x12" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="*"
                            android:textColor="@color/textred"
                            android:textSize="@dimen/x12" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/bg_gonghuoshang_edit"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <EditText
                            android:id="@+id/et_MC"
                            android:layout_width="0dp"
                            android:layout_height="@dimen/y23"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/x5"
                            android:layout_weight="1"
                            android:background="@null"
                            android:hint="必填，不区分大小写"
                            android:singleLine="true"
                            android:textColor="@color/text333"
                            android:textColorHint="@color/text999"
                            android:textSize="@dimen/x11" />


                    </LinearLayout>


                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/x10"
                android:layout_marginTop="@dimen/y5"
                android:layout_marginRight="@dimen/x20"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">

                    <TextView
                        android:layout_width="@dimen/x50"
                        android:layout_height="wrap_content"
                        android:text="简码"
                        android:textSize="@dimen/x12" />


                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/bg_gonghuoshang_edit"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <EditText
                            android:id="@+id/et_JM"
                            android:layout_width="0dp"
                            android:layout_height="@dimen/y23"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/x5"
                            android:layout_weight="1"
                            android:background="@null"
                            android:hint="按名称首字母自动生成，可修改"
                            android:singleLine="true"
                            android:textColor="@color/text333"
                            android:textColorHint="@color/text999"
                            android:textSize="@dimen/x11" />

                    </LinearLayout>


                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/x10"
                android:layout_marginTop="@dimen/y5"
                android:layout_marginRight="@dimen/x20"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">

                    <TextView
                        android:layout_width="@dimen/x50"
                        android:layout_height="wrap_content"
                        android:text="电话"
                        android:textSize="@dimen/x12" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/bg_gonghuoshang_edit"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <EditText
                            android:id="@+id/et_Phone"
                            android:layout_width="0dp"
                            android:layout_height="@dimen/y23"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/x5"
                            android:layout_weight="1"
                            android:background="@null"
                            android:hint="请输入电话"
                            android:inputType="phone"
                            android:singleLine="true"
                            android:textColor="@color/text333"
                            android:textColorHint="@color/text999"
                            android:textSize="@dimen/x11" />

                    </LinearLayout>


                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/x10"
                android:layout_marginTop="@dimen/y5"
                android:layout_marginRight="@dimen/x20"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">

                    <TextView
                        android:layout_width="@dimen/x50"
                        android:layout_height="wrap_content"
                        android:text="主营分类"
                        android:textSize="@dimen/x12" />


                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/bg_gonghuoshang_edit"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <EditText
                            android:id="@+id/yiji"

                            android:layout_width="0dp"
                            android:layout_height="@dimen/y23"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/x5"
                            android:layout_weight="1"
                            android:background="@null"
                            android:gravity="center_vertical"
                            android:hint=""
                            android:singleLine="true"
                            android:textColor="@color/text333"
                            android:textColorHint="@color/text999"
                            android:textSize="@dimen/x12" />

                        <FrameLayout
                            android:id="@+id/ll_pro_fl1"
                            android:layout_width="@dimen/x25"
                            android:layout_height="@dimen/x25"
                            android:visibility="visible">

                            <ImageView
                                android:layout_width="@dimen/x15"
                                android:layout_height="@dimen/x15"
                                android:layout_gravity="center"
                                android:background="@mipmap/ic_launcher" />

                        </FrameLayout>

                    </LinearLayout>


                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/x10"
                android:layout_marginTop="@dimen/y5"
                android:layout_marginRight="@dimen/x20"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">

                    <TextView
                        android:layout_width="@dimen/x50"
                        android:layout_height="wrap_content"
                        android:text="二分类"
                        android:textSize="@dimen/x12" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/bg_gonghuoshang_edit"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <EditText
                            android:id="@+id/erji"
                            android:layout_width="0dp"
                            android:layout_height="@dimen/y23"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/x5"
                            android:layout_weight="1"
                            android:background="@null"
                            android:gravity="center_vertical"
                            android:hint=""
                            android:singleLine="true"
                            android:textColor="@color/text333"
                            android:textColorHint="@color/text999"
                            android:textSize="@dimen/x12" />

                        <FrameLayout
                            android:id="@+id/ll_pro_fl2"
                            android:layout_width="@dimen/x25"
                            android:layout_height="@dimen/x25"
                            android:visibility="visible">

                            <ImageView
                                android:layout_width="@dimen/x15"
                                android:layout_height="@dimen/x15"
                                android:layout_gravity="center"
                                android:background="@mipmap/ic_launcher" />

                        </FrameLayout>

                    </LinearLayout>


                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/x10"
                android:layout_marginTop="@dimen/y5"
                android:layout_marginRight="@dimen/x20"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">

                    <TextView
                        android:layout_width="@dimen/x50"
                        android:layout_height="wrap_content"
                        android:text="三分类"
                        android:textSize="@dimen/x12" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/bg_gonghuoshang_edit"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <EditText
                            android:id="@+id/sanji"
                            android:layout_width="0dp"
                            android:layout_height="@dimen/y23"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/x5"
                            android:layout_weight="1"
                            android:background="@null"
                            android:gravity="center_vertical"
                            android:hint=""
                            android:singleLine="true"
                            android:textColor="@color/text333"
                            android:textColorHint="@color/text999"
                            android:textSize="@dimen/x12" />

                        <FrameLayout
                            android:id="@+id/ll_pro_fl3"
                            android:layout_width="@dimen/x25"
                            android:layout_height="@dimen/x25"
                            android:visibility="visible">

                            <ImageView
                                android:layout_width="@dimen/x15"
                                android:layout_height="@dimen/x15"
                                android:layout_gravity="center"
                                android:background="@mipmap/ic_launcher" />

                        </FrameLayout>


                    </LinearLayout>


                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/x10"
                android:layout_marginTop="@dimen/y5"
                android:layout_marginRight="@dimen/x20"
                android:layout_marginBottom="@dimen/x20"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:layout_width="@dimen/x50"
                        android:layout_height="wrap_content"
                        android:layout_gravity="start"
                        android:paddingTop="@dimen/x10"
                        android:text="备注"
                        android:textSize="@dimen/x12" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/bg_gonghuoshang_edit">

                        <EditText
                            android:id="@+id/et_beizhu"
                            android:layout_width="0dp"
                            android:layout_height="@dimen/y80"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/x5"
                            android:layout_weight="1"
                            android:background="@null"
                            android:gravity="start"
                            android:paddingTop="@dimen/x10"
                            android:singleLine="true"
                            android:textColor="@color/text333"
                            android:textColorHint="@color/text999"
                            android:textSize="@dimen/x12" />

                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>

        </LinearLayout>

    </ScrollView>

</LinearLayout>
